#include <tunables/global>

/usr/bin/evince {
  #include <abstractions/audio>
  #include <abstractions/base>
  #include <abstractions/cups-client>
  #include <abstractions/dbus-session-strict>
  #include <abstractions/fonts>
  #include <abstractions/freedesktop.org>
  #include <abstractions/gnome>
  #include <abstractions/nameservice>
  #include <abstractions/consoles>

  /**.pdf r,
  /proc/*/comm r,
  /proc/*/fd/ r,
  /run/firejail/mnt/fslogger r,
  /usr/bin/evince mr,
  /usr/share/cursors/** r,
  /usr/share/evince/icons/ r,
  /usr/share/evince/icons/hicolor/16x16/actions/ r,
  /usr/share/evince/icons/hicolor/16x16/mimetypes/ r,
  /usr/share/evince/icons/hicolor/22x22/actions/ r,
  /usr/share/evince/icons/hicolor/22x22/mimetypes/ r,
  /usr/share/evince/icons/hicolor/24x24/actions/ r,
  /usr/share/evince/icons/hicolor/24x24/mimetypes/ r,
  /usr/share/evince/icons/hicolor/32x32/actions/ r,
  /usr/share/evince/icons/hicolor/32x32/mimetypes/ r,
  /usr/share/evince/icons/hicolor/48x48/actions/ r,
  /usr/share/evince/icons/hicolor/scalable/actions/ r,
  /usr/share/evince/icons/hicolor/scalable/mimetypes/ r,
  /usr/share/glib-2.0/schemas/gschemas.compiled r,
  /usr/share/poppler/cMap/ r,
  /usr/share/poppler/cidToUnicode/ r,
  /usr/share/poppler/nameToUnicode/ r,
  /usr/share/poppler/nameToUnicode/* r,
  /usr/share/poppler/unicodeMap/ r,

  owner /tmp/** rw,
  owner @{HOME}/.Xauthority r,
  owner @{HOME}/.cache/dconf/user rw,
  owner @{HOME}/.config/dconf/user r,
  owner @{HOME}/.config/fontconfig/fonts.conf r,
  owner @{HOME}/.config/pulse/ rwk,
  owner @{HOME}/.config/pulse/cookie rk,
  owner @{HOME}/.cups/lpoptions r,
  owner @{HOME}/.local/share/applications/ r,
  owner @{HOME}/.local/share/applications/mimeapps.list r,
  owner @{HOME}/.local/share/applications/mimeinfo.cache r,
  owner @{HOME}/.local/share/applications/userapp-Thunderbird-*.desktop r,
  owner @{HOME}/.local/share/gvfs-metadata/*.log r,
  owner @{HOME}/.local/share/gvfs-metadata/home r,
  owner @{HOME}/.local/share/gvfs-metadata/root r,
  owner @{HOME}/.local/share/icons/ r,
  owner @{HOME}/.local/share/icons/hicolor/128x128/apps/ r,
  owner @{HOME}/.local/share/icons/hicolor/16x16/apps/ r,
  owner @{HOME}/.local/share/icons/hicolor/256x256/apps/ r,
  owner @{HOME}/.local/share/icons/hicolor/32x32/apps/ r,
  owner @{HOME}/.local/share/icons/hicolor/48x48/apps/ r,
  owner @{HOME}/.local/share/icons/hicolor/64x64/apps/ r,
  owner @{HOME}/.local/share/icons/hicolor/96x96/apps/ r,
  owner @{HOME}/.local/share/mime/mime.cache r,
  owner @{HOME}/.local/share/recently-used.xbel rw,
  owner @{HOME}/.local/share/recently-used.xbel.* rw,
  owner @{HOME}/.pulse/ r,

}
