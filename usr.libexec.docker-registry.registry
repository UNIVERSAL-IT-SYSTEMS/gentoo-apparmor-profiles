#include <tunables/global>

profile docker-registry /usr/libexec/docker-registry/registry {
  #include <abstractions/base>

  @{PROC}/sys/kernel/hostname r,
  @{PROC}/sys/net/core/somaxconn r,

  @{DATA_SRV}/docker/registry/certs/* r,

  /etc/docker/registry/config.yml r,
  /usr/libexec/docker-registry/registry mr,

}
